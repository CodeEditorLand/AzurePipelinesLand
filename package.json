{
  "name": "azure-pipelines",
  "displayName": "Azure Pipelines",
  "version": "0.0.1",
  "private": "false",
  "description": "Syntax highlighting, IntelliSense, and more for Azure Pipelines YAML",
  "categories": [
    "Programming Languages",
    "Formatters",
    "Azure"
  ],
  "keywords": [
    "YAML",
    "Azure Pipelines",
    "continuous integration",
    "CI/CD"
  ],
  "homepage": "https://github.com/CodeEditorLand/CodeEditorLand#readme",
  "bugs": {
    "url": "https://github.com/CodeEditorLand/CodeEditorLand/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CodeEditorLand/CodeEditorLand.git"
  },
  "license": "MIT",
  "author": {
    "name": "Nikola Hristov",
    "email": "nikola@nikolahristov.tech",
    "url": "https://nikolahristov.tech"
  },
  "publisher": "ms-azure-devops",
  "type": "module",
  "main": "./dist/extension",
  "scripts": {
    "compile": "webpack --mode production --progress --color",
    "compile:dev": "webpack --mode development --progress --color",
    "compile:test": "npm run compile:dev && tsc -p ./",
    "prepublishOnly": "TypeScriptESBuild Source/**/*.ts",
    "test": "npm run compile:test && node ./out/test/runTest.js",
    "unittest": "npm run compile:test && mocha -u tdd out/unittest/**/*.js",
    "vscode:prepublish": "npm run compile",
    "watch": "webpack --mode development --progress --color --watch"
  },
  "contributes": {
    "commands": [
      {
        "command": "azure-pipelines.configure-pipeline",
        "title": "Configure Pipeline",
        "category": "Azure Pipelines"
      }
    ],
    "configuration": {
      "title": "Azure Pipelines",
      "properties": {
        "azure-pipelines.configure": {
          "type": "boolean",
          "default": true,
          "description": "Enable 'Configure Pipeline' feature",
          "order": 0
        },
        "azure-pipelines.1ESPipelineTemplatesSchemaFile": {
          "type": "boolean",
          "default": false,
          "description": "Use 1ES Pipeline Template schema file",
          "markdownDescription": "If enabled, [1ES Pipeline Template schema file](https://aka.ms/1espt) will take precedence over custom schema file when user is signed in with `@microsoft.com` account and the schema is available in the ADO organization."
        },
        "azure-pipelines.customSchemaFile": {
          "type": "string",
          "description": "Use a different schema file",
          "scope": "machine-overridable"
        }
      }
    },
    "configurationDefaults": {
      "[azure-pipelines]": {
        "editor.insertSpaces": true,
        "editor.tabSize": 2,
        "editor.quickSuggestions": {
          "other": true,
          "comments": false,
          "strings": true
        },
        "editor.autoIndent": "full"
      }
    },
    "grammars": [
      {
        "language": "azure-pipelines",
        "scopeName": "source.yaml",
        "path": "./syntaxes/yaml.tmLanguage.json"
      }
    ],
    "languages": [
      {
        "id": "azure-pipelines",
        "configuration": "./language-configuration.json",
        "filenamePatterns": [
          "azure-pipelines.yml",
          "azure-pipelines.yaml",
          ".azure-pipelines.yml",
          ".azure-pipelines.yaml",
          "azure-pipelines/**/*.yml",
          "azure-pipelines/**/*.yaml",
          ".azure-pipelines/**/*.yml",
          ".azure-pipelines/**/*.yaml",
          ".vsts-ci.yml",
          "vsts-ci.yml"
        ],
        "aliases": [
          "Azure Pipelines"
        ]
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "azure-pipelines.configure-pipeline",
          "group": "Azure Pipelines",
          "when": "explorerResourceIsFolder == true"
        }
      ]
    }
  },
  "activationEvents": [
    "onLanguage:azure-pipelines",
    "onCommand:azure-pipelines.configure-pipeline"
  ],
  "dependencies": {
    "@azure/arm-appservice": "14.0.0",
    "@azure/arm-resources": "5.2.0",
    "@azure/arm-subscriptions": "5.1.0",
    "@azure/ms-rest-azure-env": "2.0.0",
    "@azure/ms-rest-nodeauth": "3.1.1",
    "@vscode/extension-telemetry": "0.8.5",
    "azure-devops-node-api": "12.1.0",
    "azure-pipelines-language-server": "0.7.0",
    "html-to-text": "9.0.5",
    "mustache": "4.2.0",
    "uuid": "9.0.1",
    "vscode-languageclient": "9.0.1",
    "vscode-uri": "3.0.8"
  },
  "devDependencies": {
    "@types/fs-extra": "11.0.3",
    "@types/glob": "8.1.0",
    "@types/html-to-text": "9.0.3",
    "@types/mocha": "10.0.3",
    "@types/mustache": "4.2.4",
    "@types/node": "20.8.9",
    "@types/uuid": "9.0.6",
    "@types/vscode": "~1.83.1",
    "@vscode/test-electron": "2.3.6",
    "ajv": "8.12.0",
    "assert": "2.1.0",
    "copy-webpack-plugin": "11.0.0",
    "glob": "10.3.10",
    "mocha": "10.2.0",
    "ts-loader": "9.5.0",
    "ts-node": "10.9.1",
    "tslint": "5.20.1",
    "typescript": "5.2.2",
    "typescript-esbuild": "0.3.1",
    "webpack": "5.89.0",
    "webpack-cli": "5.1.4"
  },
  "extensionDependencies": [
    "ms-vscode.azure-account"
  ],
  "engines": {
    "node": ">=12.20.0",
    "vscode": "1.64.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "icon": "assets/pipelines.png",
  "galleryBanner": {
    "color": "#D4DCEC",
    "theme": "light"
  },
  "aiKey": "AIF-d9b70cd4-b9f9-4d70-929b-a071c400b217",
  "capabilities": {
    "untrustedWorkspaces": {
      "supported": "limited",
      "restrictedConfigurations": [
        "azure-pipelines.customSchemaFile"
      ]
    }
  },
  "tags": [
    "azure-pipelines",
    "Azure Pipelines",
    "YAML"
  ]
}
